local a=game:GetService("Players")local b=a.LocalPlayer;local function c()local d=workspace:WaitForChild("Players")local e=d:FindFirstChild("Killers")if e then local f=e:FindFirstChild(b.Name)if f then local g=f:FindFirstChild("SpeedMultipliers")if g then local h=g:FindFirstChild("DirectionalMovement")if h then return h end end end end;local i=d:FindFirstChild("Survivors")if i then local j=i:FindFirstChild(b.Name)if j then local g=j:FindFirstChild("SpeedMultipliers")if g then local h=g:FindFirstChild("DirectionalMovement")if h then return h end end end end;return nil end;while wait()do local h=c()if h then h:Destroy()end end
